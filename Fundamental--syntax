#get all users: 
get-ADUser -filter

Sample script:
$groups = Get-ADGroup -LDAPFilter "(name=*)"
$groups

gcm *member*
get-ADGroupmember -Identity Administrators

foreach ($group in $groups) {
    Get-ADGroupMember -Identity $group
}

alias echo
foreach ($group in $group) {
    write-output "...."
    write-output "...."
    Get-ADGroupMember -Identity $group
    write-output "-----end----"
    
}
